{
  "name": "gcull",
  "version": "1.0.0",
  "description": "interactive git branch deleter",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "tsc",
    "test": "vitest --coverage",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,json}": "prettier --write",
    "*.{js,ts}": "npm run lint --fix"
  },
  "bin": {
    "gcull": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krnsk0/cull.git"
  },
  "keywords": [
    "git",
    "cli",
    "interactive",
    "branch"
  ],
  "author": "jkurinsky@fastmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krnsk0/cull/issues"
  },
  "homepage": "https://github.com/krnsk0/cull#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-istanbul": "^0.34.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vitest": "^0.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
