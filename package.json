{
  "name": "gcull",
  "version": "1.0.0",
  "description": "interactive git branch deleter",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "tsc",
    "test": "vitest --coverage",
    "lint:files": "eslint --ignore-path ./.lintignore",
    "lint": "npm run lint:files .",
    "format:files": "prettier --ignore-path ./.lintignore --write",
    "format": "npm run format:files .",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json}": "npm run format:files"
  },
  "bin": {
    "gcull": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krnsk0/cull.git"
  },
  "keywords": [
    "git",
    "cli",
    "interactive",
    "branch"
  ],
  "author": "jkurinsky@fastmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krnsk0/cull/issues"
  },
  "homepage": "https://github.com/krnsk0/cull#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-istanbul": "^0.34.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vitest": "^0.3.1",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
